{
  "manifest_version": 3,
  "name": "Diagnostic Flash RGAA",
  "version": "1.0.1",
  "description": "Extension pour réaliser les diagnostics flash d'accessibilité",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
